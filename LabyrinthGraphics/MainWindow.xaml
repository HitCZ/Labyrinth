<Window x:Class="LabyrinthGraphics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabyrinthGraphics"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">

    <Grid>
        <Canvas x:Name="canvas" Height="650" Width="650" />
        <TextBox x:Name="textBoxInput" HorizontalAlignment="Left" Height="23" Margin="1045,157,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonGenerate" Content="Generuj" HorizontalAlignment="Left" Margin="1089,218,0,0" VerticalAlignment="Top" Width="76" Click="buttonGenerate_Click"/>
        <Label x:Name="labelInfo" Content="label" HorizontalAlignment="Left" Margin="1035,126,0,0" VerticalAlignment="Top"/>
        <Button x:Name="buttonSave" Content="Uložit" HorizontalAlignment="Left" Margin="1138,264,0,0" VerticalAlignment="Top" Width="76" Click="buttonSave_Click"/>
        <Button x:Name="buttonLoad" Content="Načíst" HorizontalAlignment="Left" Margin="1035,264,0,0" VerticalAlignment="Top" Width="75" Click="buttonLoad_Click"/>
    </Grid>
</Window>
